extends layout
block content
    .w3-container
        .w3-card-4
            header.w3-container.w3-red
                h3 Tese Recebida
            .w3-container
                br
                table.w3-table-all
                    tr
                        th Nome
                        td= tese.nome
                    tr
                        th Identificador
                        td= tese.numero
                    tr
                        th Curso
                        td= tese.curso
                    tr
                        th Instituição
                        td= tese.instituicao
                    tr
                        th Título
                        td= tese.titulo
                    tr
                        th Data
                        td= tese.data
                    tr
                        th Tipo de Tese
                        td= tese.tipo
                p
                    a(href="/") Voltar
            footer.w3-container.w3-red
                address Gerado por teseApp